{
  "compilerOptions": {
    "target": "ES5", 
    "jsx": "react",
    "allowJs": true,
    "checkJs": true,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true, 
    "strict": true,
    "outDir": "./build" 
  },
  "include": [
    "src/**/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}